{
  "name": "cdk-multi-module-project-template",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "test": "jest",
    "clean": "yarn clean:dist && yarn clean:modules",
    "clean:dist": "find . -type d -name 'dist' -prune -exec rm -rf '{}' '+'",
    "clean:modules": "find . -type d -name 'node_modules' -prune -exec rm -rf '{}' '+' "
  },
  "workspaces": {
    "packages": [
      "api/*",
      "shared"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.12",
    "@types/node": "11.13.9",
    "@aws-cdk/aws-ec2": "0.30.0",
    "@aws-cdk/aws-lambda": "0.30.0",
    "@aws-cdk/aws-apigateway": "0.30.0",
    "@aws-cdk/cdk": "0.30.0",
    "aws-sdk": "2.448.0",
    "cdk-infra-properties": "0.1.9",
    "jest": "24.7.1",
    "jest-environment-jsdom": "24.7.1",
    "lerna": "3.13.4",
    "node": "8.15.1",
    "prettier": "1.17.0",
    "ts-jest": "24.0.2",
    "ts-loader": "5.4.5",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5",
    "webpack": "4.30.0"
  },
  "dependencies": {
    "lodash": "4.17.11",
    "source-map-support": "0.5.12"
  },
  "engines": {
    "node": ">=8.10.0 <=v8.16.0"
  }
}
